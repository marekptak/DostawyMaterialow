@model IEnumerable<Dostawa>

@{
    ViewData["Title"] = "Home Page";
}

    <div class="text-center">
        <h1 class="display-4">Dostawy materiałów</h1>
        <p>Zestawienie wszystkich dostaw materiałów.</p>
    </div>

<div>
    @foreach(var dostawa in Model)
    {
    <div>
        <h2>Średnica: @dostawa.Srednica</h2>
        <h5>Gatunek: @dostawa.Gatunek</h5>
        <h5>Masa: @dostawa.Masa</h5>
        <h5>Data dostawy: @dostawa.DataDostawy</h5>
        <h6>Długość: @dostawa.Dlugosc</h6>
        <h6>Dostawca: @dostawa.Dostawca</h6>
        <h6>Wytop: @dostawa.Wytop</h6>
        <br/>

    </div>
    }
</div>

